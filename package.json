{
  "name": "junit-runner",
  "main": "./lib/junit-runner",
  "version": "0.0.0",
  "description": "Run and view JUnit tests",
  "keywords": [
    "junit",
    "java",
    "run",
    "test"
  ],
  "activationCommands": {
    "atom-text-editor": "junit-runner:run"
  },
  "repository": "https://github.com/50Wliu/junit-runner",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "^3.15.0"
  },
  "configSchema": {
    "classpath": {
      "description": "Classpath to use when compiling and running.",
      "default": "",
      "type": "string"
    },
    "javaArgs": {
      "description": "Arguments to pass to the `java` process.",
      "default": "",
      "type": "string"
    },
    "javacArgs": {
      "description": "Arguments to pass to the `javac` process.",
      "default": "",
      "type": "string"
    }
  }
}
